{
  "name": "vsq.ts",
  "version": "0.0.1",
  "description": "TypeScript of https://github.com/abetomo/Very-simple-queue",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "standard",
    "test": "npm run lint && npm run test-typings && npm run unit",
    "test-typings": "tsc --noEmit",
    "unit": "jest -w 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abetomo/vsq.ts.git"
  },
  "author": "abetomo",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/abetomo/vsq.ts/issues"
  },
  "homepage": "https://github.com/abetomo/vsq.ts#readme",
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/node": "^11.13.10",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.8.0",
    "standard": "^12.0.1",
    "typescript": "^3.4.5",
    "typescript-eslint-parser": "^22.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ]
  },
  "dependencies": {
    "commander": "^2.20.0",
    "uuid": "^3.3.2"
  },
  "engines": {
    "node": ">= 6.0.0"
  }
}

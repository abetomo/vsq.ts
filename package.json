{
  "name": "vsq.ts",
  "version": "0.0.1",
  "description": "TypeScript of https://github.com/abetomo/Very-simple-queue",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "standard",
    "test": "npm run lint && npm run unit",
    "unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abetomo/vsq.ts.git"
  },
  "author": "abetomo",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/abetomo/vsq.ts/issues"
  },
  "homepage": "https://github.com/abetomo/vsq.ts#readme",
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.53",
    "eslint-plugin-typescript": "^0.8.1",
    "jest": "^21.2.1",
    "standard": "^10.0.3",
    "typescript": "^2.6.2",
    "typescript-eslint-parser": "^9.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ]
  }
}

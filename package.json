{
  "name": "vsq.ts",
  "version": "0.0.1",
  "description": "TypeScript of https://github.com/abetomo/Very-simple-queue",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "standard",
    "test": "npm run lint && npm run test-typings && npm run unit",
    "test-typings": "tsc --noEmit",
    "unit": "jest -w 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abetomo/vsq.ts.git"
  },
  "author": "abetomo",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/abetomo/vsq.ts/issues"
  },
  "homepage": "https://github.com/abetomo/vsq.ts#readme",
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "jest": "^25.1.0",
    "standard": "^14.3.1",
    "typescript": "^3.8.2"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "dependencies": {
    "commander": "^4.1.1",
    "uuid": "^7.0.0"
  },
  "engines": {
    "node": ">= 6.0.0"
  }
}
